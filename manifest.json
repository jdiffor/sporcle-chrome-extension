{
    "manifest_version": 2,

    "name": "Random Sporcle Quiz",
    "description": "Opens a random quiz from Sporcle in the desired category or subcategory in a new tab",
    "version": "1.0",
    "icons": { "128": "icon_128.png" },
    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "background": {

    },
    "options_page": "options.html",
    "permissions": ["activeTab"]
}
